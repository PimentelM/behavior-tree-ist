{
  "name": "behaviourist",
  "version": "0.1.0",
  "description": "A behaviour tree library for TypeScript",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./builder": {
      "import": {
        "types": "./dist/builder.d.mts",
        "default": "./dist/builder.mjs"
      },
      "require": {
        "types": "./dist/builder.d.ts",
        "default": "./dist/builder.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "yarn build && yarn typecheck && yarn lint && yarn test"
  },
  "keywords": [
    "behaviour-tree",
    "behavior-tree",
    "ai",
    "game-ai",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "eslint": "^9.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.0.0",
    "vitest": "^3.0.0"
  }
}