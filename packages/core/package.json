{
  "name": "@behavior-tree-ist/core",
  "version": "0.1.0",
  "description": "A behaviour tree library for TypeScript",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./builder": {
      "import": {
        "types": "./dist/builder.d.mts",
        "default": "./dist/builder.mjs"
      },
      "require": {
        "types": "./dist/builder.d.ts",
        "default": "./dist/builder.js"
      }
    },
    "./tsx": {
      "import": {
        "types": "./dist/tsx.d.mts",
        "default": "./dist/tsx.mjs"
      },
      "require": {
        "types": "./dist/tsx.d.ts",
        "default": "./dist/tsx.js"
      }
    },
    "./inspector": {
      "import": {
        "types": "./dist/inspector.d.mts",
        "default": "./dist/inspector.mjs"
      },
      "require": {
        "types": "./dist/inspector.d.ts",
        "default": "./dist/inspector.js"
      }
    },
    "./activity": {
      "import": {
        "types": "./dist/activity.d.mts",
        "default": "./dist/activity.mjs"
      },
      "require": {
        "types": "./dist/activity.d.ts",
        "default": "./dist/activity.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "yarn build && yarn typecheck && yarn lint && yarn test"
  },
  "keywords": [
    "behaviour-tree",
    "behavior-tree",
    "ai",
    "game-ai",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
