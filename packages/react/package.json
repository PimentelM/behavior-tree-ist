{
  "name": "@behavior-tree-ist/react",
  "version": "0.1.0",
  "description": "React debugger component for behavior-tree-ist",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src"
  },
  "peerDependencies": {
    "@behavior-tree-ist/core": "workspace:*",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@dagrejs/dagre": "^1",
    "@xyflow/react": "^12"
  },
  "devDependencies": {
    "@behavior-tree-ist/core": "workspace:*",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "react": "^18",
    "react-dom": "^18"
  },
  "keywords": [
    "behaviour-tree",
    "behavior-tree",
    "react",
    "debugger",
    "inspector"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
